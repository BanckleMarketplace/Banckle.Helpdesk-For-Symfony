<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="bancklehelpdesk.api" class="Banckle\Bundle\HelpdeskBundle\Service\BanckleHelpdeskWrapper" public="true">
            <argument>%apiKey%</argument>
            <argument>%banckleAccountUri%</argument>
            <argument>%banckleHelpdeskUri%</argument>
        </service>
    </services>
	
</container>